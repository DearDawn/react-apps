  "license": "ISC",
  "devDependencies": {
    "@parcel/config-default": "^2.10.3",
    "@parcel/core": "^2.10.3",
    "@parcel/transformer-less": "^2.10.3",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "parcel": "^2.10.3",
    "process": "^0.11.10",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "pixi.js": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7"
  }

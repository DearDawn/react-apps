{
  "name": "react-apps",
  "version": "1.0.12",
  "description": "React 多页应用模板",
  "scripts": {
    "prestart": "node prebuild.js",
    "start": "npm run clean && parcel src/pages/*/index.html -p 7777",
    "start:server": "npm run clean && parcel src/pages/*/index.html -p 7798",
    "start:api": "npm run clean && parcel src/pages/*/index.html -p 7777 --open",
    "root": "parcel src/pages/*.html -p 7777",
    "prebuild": "node prebuild.js",
    "build": "npm run clean && parcel build src/pages/*.html --public-url ./ --no-source-maps && parcel build src/pages/*/*.html --public-url ../ --no-source-maps",
    "build:analyze": "parcel build src/pages/*.html --public-url ./ --no-source-maps && parcel build src/pages/*/*.html --public-url ../ --no-source-maps --reporter @parcel/reporter-bundle-analyzer",
    "clean": "run-script-os",
    "clean:win32": "(rmdir /s /q .parcel-cache || (echo Failed to delete .parcel-cache)) && (rmdir /s /q dist  || (echo Failed to delete dist))",
    "clean:default": "rm -rf .parcel-cache;rm -rf dist"
  },
  "alias": {
    "pixi.js": "./node_modules/pixi.js/dist/pixi.min.mjs",
    "pixi6.js": "./node_modules/pixi6.js/dist/esm/pixi.min.mjs",
    "pixi-live2d-display": "./node_modules/pixi-live2d-display/dist/cubism4.es.js",
    "pixi-live2d-display/extra": "./node_modules/pixi-live2d-display/dist/extra.es.js"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/config-default": "^2.12.0",
    "@parcel/core": "^2.12.0",
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/reporter-bundle-analyzer": "^2.12.0",
    "@parcel/transformer-less": "^2.12.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "buffer": "^6.0.3",
    "http-proxy-middleware": "^2.0.6",
    "parcel": "^2.12.0",
    "postcss": "^8.4.32",
    "postcss-modules": "^4.3.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pixi-live2d-display": "^0.4.0",
    "pixi.js": "^7.3.2",
    "pixi6.js": "npm:pixi.js@^6.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "run-script-os": "^1.1.6",
    "socket.io-client": "^4.7.2",
    "sweet-me": "^0.0.156",
    "vconsole": "^3.15.1"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7"
  }
}
